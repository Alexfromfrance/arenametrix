<%#= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th>Numéro de billet</th>
      <th>Numéro de réservation</th>
      <th>Date representation</th>
      <th>Heure representation</th>
      <th>Cle spectacle</th>
      <th>Spectacle</th>
      <th>Cle representation</th>
      <th>Representation</th>
      <th>Date representation</th>
      <th>Heure representation</th>
      <th>Date fin representation</th>
      <th>Heure fin representation</th>
      <th>Prix</th>
      <th>Type de produit</th>
      <th>Filiere de vente</th>
      <th>Nom</th>
      <th>Prenom</th>
      <th>Email</th>
      <th>Adresse</th>
      <th>Code postal</th>
      <th>Pays</th>
      <th>Age</th>
      <th>Sexe</th>

    </tr>
  </thead>
<tbody>
    <% @bookings.each do |booking| %>
    <tr>
      <td><%=booking.num_billet %></td>
      <td><%=booking.num_resa %></td>
      <td><%=booking.date_resa %></td>
      <td><%=booking.heure_resa %></td>
      <td><%=booking.cle %></td>
      <td><%=booking.nom_spectacle %></td>
      <td><%=booking.cle_representation %></td>
      <td><%=booking.representation %></td>
      <td><%=booking.date_representation %></td>
      <td><%=booking.heure_representation %></td>
      <td><%=booking.date_fin_representation %></td>
      <td><%=booking.heure_fin_representation %></td>
      <td><%=booking.prix %></td>
      <td><%=booking.type_produit %></td>
      <td><%=booking.filiere %></td>
      <td><%=booking.nom %></td>
      <td><%=booking.prenom %></td>
      <td><%=booking.email %></td>
      <td><%=booking.adresse %></td>
      <td><%=booking.postal %></td>
      <td><%=booking.pays %></td>
      <td><%=booking.age %></td>
      <td><%=booking.sexe %></td>
    </tr>
    <% end %>
</tbody>
</table>

<div>
  <h1>Import CSV</h1>
  <%= form_tag import_bookings_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import CSV" %>
  <% end %>
</div>

<div class="goback">
  <%= link_to "<< Page admin",  admindata_path %>
</div>

